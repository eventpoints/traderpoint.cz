{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "beberlei/doctrineextensions": "^1.5",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.3",
    "easycorp/easyadmin-bundle": "^4.26.5",
    "endroid/qr-code-bundle": "^6.0.0",
    "giggsey/libphonenumber-for-php-lite": "^9.0",
    "intervention/image": "^3.9",
    "intervention/image-symfony": "*",
    "jdenticon/jdenticon": "^1.0",
    "jsor/doctrine-postgis": "*",
    "knplabs/knp-paginator-bundle": "^6.6",
    "knpuniversity/oauth2-client-bundle": "*",
    "lcobucci/clock": "^3.5",
    "lcobucci/jwt": "^5.2",
    "league/oauth2-facebook": "^2.2.0",
    "league/oauth2-google": "^4.0.0",
    "nesbot/carbon": "^3.8",
    "odolbeau/phone-number-bundle": "*",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^1.33",
    "stripe/stripe-php": "*",
    "symfony/asset": "^7.3",
    "symfony/asset-mapper": "^7.3",
    "symfony/cache": "^7.3",
    "symfony/cache-contracts": "^3.5",
    "symfony/console": "^7.3",
    "symfony/debug-bundle": "^7.3",
    "symfony/doctrine-messenger": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/expression-language": "^7.3",
    "symfony/flex": "^2",
    "symfony/form": "^7.3",
    "symfony/forty-six-elks-notifier": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/intl": "^7.3",
    "symfony/mailer": "^7.3",
    "symfony/maker-bundle": "^1.61",
    "symfony/mime": "^7.3",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "^7.3",
    "symfony/polyfill-intl-messageformatter": "^1.31",
    "symfony/process": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/property-info": "^7.3",
    "symfony/rate-limiter": "^7.3",
    "symfony/runtime": "^7.3",
    "symfony/security-bundle": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/stimulus-bundle": "^2.20",
    "symfony/string": "^7.3",
    "symfony/translation": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/uid": "^7.3",
    "symfony/ux-autocomplete": "^2.21",
    "symfony/ux-dropzone": "^2.30",
    "symfony/ux-icons": "^2.2",
    "symfony/ux-leaflet-map": "^2.30",
    "symfony/ux-map": "^2.30",
    "symfony/ux-turbo": "^2.20",
    "symfony/validator": "^7.3",
    "symfony/web-link": "^7.3",
    "symfony/yaml": "^7.3",
    "symfonycasts/verify-email-bundle": "^1.17",
    "twig/cssinliner-extra": "*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/intl-extra": "^3.13",
    "twig/twig": "^2.12|^3.0",
    "vich/uploader-bundle": "^2.8"
  },
  "config": {
    "allow-plugins": {
      "endroid/installer": false,
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "fix-cs": "vendor/bin/ecs check --fix --ansi",
    "check-cs": "vendor/bin/ecs check --ansi",
    "phpstan": "php -d memory_limit=256M vendor/bin/phpstan analyze",
    "rector-dry": "vendor/bin/rector p --dry-run --ansi",
    "rector": "vendor/bin/rector p --ansi",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^7.3"
    }
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.6",
    "fakerphp/faker": "^1.23",
    "phpstan/phpstan": "^1.12",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^1.2",
    "symfony/browser-kit": "^7.3",
    "symfony/css-selector": "^7.3",
    "symfony/phpunit-bridge": "^7.1",
    "symfony/stopwatch": "^7.3",
    "symfony/web-profiler-bundle": "^7.3",
    "symplify/config-transformer": "^12.3",
    "symplify/easy-coding-standard": "^12.3"
  }
}
